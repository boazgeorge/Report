<Window x:Class="Report.ErrorLogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Report"
        mc:Ignorable="d"
        Title="ErrorLogWindow" Height="505.8" Width="589.6">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="335" Margin="10,52,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="557" IsReadOnly="True"/>
        <Button x:Name="button" Content="Close" HorizontalAlignment="Left" Margin="492,401,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="116,21,0,0" VerticalAlignment="Top" Width="451" SelectionChanged="comboBox_SelectionChanged"/>
        <Label x:Name="label" Content="File" HorizontalAlignment="Left" Margin="10,21,0,0" VerticalAlignment="Top" Width="101"/>
        <Button x:Name="button1" Content="Fix Errors" HorizontalAlignment="Left" Margin="364,401,0,0" VerticalAlignment="Top" Width="94" Click="button1_Click"/>

    </Grid>
</Window>
